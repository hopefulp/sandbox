#create crossovers
load pdb na 65_Strand1.pdb
load pdb na 65_Strand2.pdb
trans 2  0  19  0.0
rotate 2 3 65
rotate 1 3 275

set zoom 2
# Crossover #1
modify ioffs 0.0 -60.000000
modify ioffs 0.0 -60.000000
modify ioffs 0.0 -60.000000
modify ioffs 0.0 -60.000000
modify ioffs 0.0 -60.000000
nick h1:5:1
nick h2:5:1
link h1:5:1 h2:6:1
link h2:5:1 h1:6:1

# Crossover #2
modify ioffs 0.0 60.000000
modify ioffs 0.0 60.000000
nick h1:12:2
nick h2:12:2
link h1:12:2 h2:11:2
link h2:12:2 h1:11:2

# Crossover #3
modify ioffs 0.0 60.000000
modify ioffs 0.0 60.000000
nick h2:16:1
nick h1:16:1
link h2:16:1 h1:17:1
link h1:16:1 h2:17:1

# Crossover #4
modify ioffs 0.0 60.000000
modify ioffs 0.0 60.000000
nick h1:23:2
nick h2:23:2
link h1:23:2 h2:22:2
link h2:23:2 h1:22:2

# Crossover #5
modify ioffs 0.0 60.000000
modify ioffs 0.0 60.000000
nick h1:27:1
nick h2:27:1
link h1:27:1 h2:28:1
link h2:27:1 h1:28:1

# Crossover #6
modify ioffs 0.0 60.000000
modify ioffs 0.0 60.000000
nick h1:35:2
nick h2:35:2
link h1:35:2 h2:34:2
link h2:35:2 h1:34:2

modify ioffs 0.0 -60.000000
modify ioffs 0.0 -60.000000
modify ioffs 0.0 -60.000000
modify ioffs 0.0 -60.000000
modify ioffs 0.0 -60.000000
modify ioffs 0.0 -60.000000
modify ioffs 0.0 -60.000000
modify ioffs 0.0 -60.000000
modify ioffs 0.0 -60.000000
modify ioffs 0.0 -60.000000
modify ioffs 0.0 -60.000000

reset
set zoom 1
# This will color the different strand of the molecule
set depth_cueing on
set thick 1
set thick 2
set color m1:1:*:* blue
set color m1:2:*:* orange
set color m2:1:*:* red
set color m2:2:*:* white
set color m2:2:*:* white
# Create Pictues...
set space *
